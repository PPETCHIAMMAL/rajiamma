using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.Serialization;
using System.Text;
using System.Threading.Tasks;

namespace userDao.userException
{
    [Serializable]
    public class UserReadException : Exception
    {
        public UserReadException()
        {
        }
        public UserReadException(Exception e)
        {
        }

        public UserReadException(string? message) : base(message)
        {
        }

        public UserReadException(string? message, Exception? innerException) : base(message, innerException)
        {
        }

        protected UserReadException(SerializationInfo info, StreamingContext context) : base(info, context)
        {
        }
    }

}
