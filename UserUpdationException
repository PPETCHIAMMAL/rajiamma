using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.Serialization;
using System.Text;
using System.Threading.Tasks;

namespace userDao.userException
{
    [Serializable]
    public class UserUpdationException : Exception
    {
        public UserUpdationException()
        {
        }

        public UserUpdationException(Exception e)
        {
        }

        public UserUpdationException(string? message) : base(message)
        {
        }

        public UserUpdationException(string? message, Exception? innerException) : base(message, innerException)
        {
        }

        protected UserUpdationException(SerializationInfo info, StreamingContext context) : base(info, context)
        {
        }
    }
}
